node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'Default Maven';
    withSonarQubeEnv() {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=DevTestOpsOrganisation_devTestOps_20835d94-020d-4590-ba39-172d0d709757 -Dsonar.projectName='devTestOps'"
    }
  }
}